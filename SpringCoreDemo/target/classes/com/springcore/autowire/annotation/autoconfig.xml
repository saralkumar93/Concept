	</bean>


<bean class="com.springcore.autowire.annotation.Address" name="add">
		<property name="street" value="manish nagar"></property>
		<property name="city" value="Nagpur"></property>
	</bean>
	
	<bean class="com.springcore.autowire.annotation.Emp" name="emp1"/>








</beans> 